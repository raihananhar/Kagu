{
  "name": "orbcomm-api-server",
  "version": "1.0.0",
  "description": "ORBCOMM API Server with multi-client support and real-time WebSocket connections",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test/local-test.js",
    "test:api": "node test/api-test.js",
    "test:websocket": "node test/websocket-test.js",
    "debug": "DEBUG=* node server.js",
    "debug:orbcomm": "DEBUG=orbcomm* node server.js",
    "lint": "eslint .",
    "logs": "tail -f logs/combined.log",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [
    "orbcomm",
    "api",
    "websocket",
    "satellite",
    "iot",
    "tracking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-ws": "^5.0.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "mysql2": "^3.14.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
